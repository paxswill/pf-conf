#
# Start with the system default configuration file /etc/pf.conf
#
include "/etc/pf.conf"

#
# @DOMAIN_PREFIX@.pf anchor point
#
anchor "@DOMAIN_PREFIX@.pf"
load anchor "@DOMAIN_PREFIX@.pf" from "/etc/pf.anchors/@DOMAIN_PREFIX@.pf.rules"
