#
# REVERSE_DOMAIN.pf anchor point
#
anchor "REVERSE_DOMAIN.pf"
load anchor "REVERSE_DOMAIN.pf" from "/etc/pf.anchors/REVERSE_DOMAIN.pf.rules"
